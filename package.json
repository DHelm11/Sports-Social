{
  "name": "meteor-bootstrap4-react-router4-template",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "npm run test-server && npm run test-client",
    "test-server": "TEST_CLIENT=0 SERVER_TEST_REPORTER=\"list\" meteor test --once --driver-package meteortesting:mocha --port 3100",
    "test-client": "NODE_ENV=UNIT_TESTING jest",
    "prettier": "prettier --write \"client/**/*.js\" \"imports/**/*.js\" \"server/**/*.js\" --single-quote true --trailing-comma es5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/enzyme-config.js",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "antd": "^3.24.2",
    "bcrypt": "^3.0.6",
    "indexof": "0.0.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "mongodb": "^3.3.3",
    "popper.js": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "recompose": "^0.30.0",
    "simpl-schema": "^1.5.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "autoprefixer": "^9.7.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^74.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^5.2.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browserlist": [
          "last 2 versions"
        ]
      }
    }
  }
}
